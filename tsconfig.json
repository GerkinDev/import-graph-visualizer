{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES2019", "es2020", "DOM"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "sourceMap": true,
    "resolveJsonModule": true,
    "incremental": true
  }
}
